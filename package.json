{
  "name": "rick-tech-test-vue",
  "version": "0.0.0",
  "scripts": {
    "build": "run-p type-check build-only",
    "test:unit": "vitest run --environment jsdom",
    "serve": "vite --port 3001",
    "cy:run": "cypress run --e2e",
    "test:e2e": "start-server-and-test serve http-get://localhost:3001 'cypress run --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build-only": "vite build",
    "dev": "vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vue/cli": "^5.0.7",
    "@vue/cli-plugin-e2e-cypress": "^5.0.7",
    "babel-eslint": "^10.1.0",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.41",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.1.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^8.0.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.4",
    "vite": "^2.9.12",
    "vite-plugin-windicss": "^1.8.6",
    "vitest": "^0.15.1",
    "vue-cli-plugin-windicss": "~1.1.4",
    "vue-tsc": "^0.38.1",
    "wait-on": "^6.0.1",
    "windicss": "^3.5.6"
  }
}
